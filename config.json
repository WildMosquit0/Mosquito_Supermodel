{
  "model": {
    "weights": "yolo11n.pt",
    "task": "track",
    "conf_threshold": 0.8,
    "iou_threshold": 0.2,
    "vid_stride":10
  },
  "input": {
    "images_dir": "src/tests"
  },
  "output": {
    "output_dir": "src/tests",
    "save_animations": true
  },
  "analyze": {
    "csv_path": "src/tests/results.csv",
    "output_dir": "src/tests/plots",
    "id_OR_class": "label",
    "true_axis":"true"
  },
  "average_visits": {
    "time_intervals": 1,
    "interval_unit": "minutes",
    "fps": "30",
    "output_dir": "src/tests/plots"
  }
}